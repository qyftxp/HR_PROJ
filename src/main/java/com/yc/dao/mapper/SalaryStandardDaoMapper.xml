<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yc.dao.SalaryStandardDaoMapper">
	
	
	<!-- 根据薪酬标准信息单号查询 -->
	<select id="getSalaryStandardById" resultType="SalaryStandard" parameterType="String">
		select * from salary_standard  where standard_id = #{standard_id} 
	</select>
	
</mapper>